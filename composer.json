{
    "name": "zf2timo/tte-maintenance",
    "description": "A Module to move a Website to Maintenance Mode",
    "type": "module",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2"
    ],
    "homepage": "https://github.com/zf2timo/tteMaintenance",
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zendframework": "^2.4"
    },
    "autoload": {
        "psr-0": {
            "tteMaintenance": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpcov": "2.0.*"
    }
}
